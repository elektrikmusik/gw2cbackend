<ul class="menu-marker group">
<% _.each(Icons, function(iconsTypeObject, cat) { %>
    <li class="menu-item" data-markerCat="<%= cat %>">
        <a href="javascript:" class="menu-title"><%= Markers[cat].name %> <span class="arrow"> â–¼</span></a>
        <ul class="type-menu group">
          <li class="type-menu-item"><a href="javascript:" class="group-toggling">Toggle all markers</a></li>
          <% _.each(iconsTypeObject, function(iconsObject, type) { %>
          <li class="type-menu-item">
              <a href="javascript:" class="marker-type-link trigger" data-type="<%= type %>" data-cat="<%= cat %>" title="Click to toggle"><img src="<%= iconsObject.url %>" alt="<%= iconsObject.label %>" /><span><%= iconsObject.label %></span></a>
              <a href="javascript:" class="add button" title="Add marker"></a>
          </li> 
          <% }); %>
        </ul>
    </li>
<% }); %>
</ul>
